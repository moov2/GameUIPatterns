{
  "name": "moov2/GameUIPatterns",
  "type": "wordpress-theme",
  "description": "WordPress GameUIPatterns theme ",
  "homepage": "http://gameuipatterns.com/",
  "authors": [
    {
      "name": "Moov2",
      "email": "support+github@moov2.com",
      "homepage": "https://github.com/moov2"
    }
  ],
  "keywords": ["wordpress", "theme", "moov2"],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "require": {
    "php": ">=7",
    "composer/installers": "~1.0",
    "illuminate/support": "~5.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8.0"
  },
  "scripts": {
    "test": ["phpcs"],
    "post-create-project-cmd": [
      "Roots\\Sage\\Installer\\ComposerScript::postCreateProject"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
